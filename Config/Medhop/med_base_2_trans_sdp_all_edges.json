{
  "dataset": "medhop",
  "model_class": "HDE",
  "gnn_class": "TransformerEdge",
  "use_gating": true,
  "use_transformer_block": true,
  "use_simple_hde": true,

  "layerwise_weight_sharing": false,
  "use_switch_summariser": false,
  "use_switch_coattention": false,
  "use_switch_gnn": false,
  "gnn_aggr": "add",

  "embedder_type": "bert",
  "bert_name": "simonlevine/bioclinical-roberta-long",
  "bert_fine_tune_layers": [],
  "embedded_dims": 768,
  "transformer_heads": 8,

  "print_loss_every": 300,
  "checkpoint_every": 300,
  "num_epochs": 100,

  "include_trans_gnn_edge_types": true,
  "include_trans_gnn_edges": false,

  "initial_lr": 0.05
}